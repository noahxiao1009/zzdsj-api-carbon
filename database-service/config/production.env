# 数据库管理微服务生产环境配置

# 服务配置
DB_SERVICE_NAME=database-service
DB_SERVICE_PORT=8089
DEBUG=false

# PostgreSQL配置
POSTGRES_HOST=postgres-cluster.production.svc.cluster.local
POSTGRES_PORT=5432
POSTGRES_USER=carbon_user
POSTGRES_PASSWORD=${POSTGRES_PASSWORD}
POSTGRES_DB=carbon_production
POSTGRES_SCHEMA=public
POSTGRES_MAX_CONNECTIONS=50
POSTGRES_TIMEOUT=30

# Elasticsearch配置
ELASTICSEARCH_HOSTS=https://elasticsearch-cluster.production.svc.cluster.local:9200
ELASTICSEARCH_USERNAME=${ES_USERNAME}
ELASTICSEARCH_PASSWORD=${ES_PASSWORD}
ELASTICSEARCH_API_KEY=${ES_API_KEY}
ELASTICSEARCH_TIMEOUT=30

# Milvus配置
MILVUS_HOST=milvus-cluster.production.svc.cluster.local
MILVUS_PORT=19530
MILVUS_USERNAME=${MILVUS_USERNAME}
MILVUS_PASSWORD=${MILVUS_PASSWORD}
MILVUS_SECURE=true
MILVUS_TIMEOUT=10

# Redis配置
REDIS_HOST=redis-cluster.production.svc.cluster.local
REDIS_PORT=6379
REDIS_PASSWORD=${REDIS_PASSWORD}
REDIS_DB=0
REDIS_MAX_CONNECTIONS=100

# Nacos配置
NACOS_SERVERS=nacos-cluster.production.svc.cluster.local:8848
NACOS_NAMESPACE=production
NACOS_GROUP=CARBON_GROUP
NACOS_USERNAME=${NACOS_USERNAME}
NACOS_PASSWORD=${NACOS_PASSWORD}
NACOS_SERVICE_NAME=database-service
NACOS_SERVICE_IP=${POD_IP}
NACOS_SERVICE_PORT=8089

# RabbitMQ配置
RABBITMQ_HOST=rabbitmq-cluster.production.svc.cluster.local
RABBITMQ_PORT=5672
RABBITMQ_USERNAME=${RABBITMQ_USERNAME}
RABBITMQ_PASSWORD=${RABBITMQ_PASSWORD}
RABBITMQ_VHOST=/carbon

# 健康检查配置
HEALTH_CHECK_ENABLED=true
HEALTH_CHECK_INTERVAL=30

# 监控配置
MONITORING_ENABLED=true
METRICS_PORT=9090
LOG_LEVEL=WARNING

# 网关注册配置
GATEWAY_ENABLED=true
GATEWAY_URL=http://gateway-service.production.svc.cluster.local:8080
GATEWAY_TOKEN=${GATEWAY_TOKEN} 