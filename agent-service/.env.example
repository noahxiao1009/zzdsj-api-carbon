# 基础配置
DEBUG=false
HOST=0.0.0.0
PORT=8081
WORKERS=1
LOG_LEVEL=INFO

# 数据库配置
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/agent_db

# Redis配置
REDIS_URL=redis://localhost:6379/1

# AI模型API密钥
# OPENAI_API_KEY=your_openai_api_key_here
# ANTHROPIC_API_KEY=your_anthropic_api_key_here
# ZHIPU_API_KEY=your_zhipu_api_key_here
# MOONSHOT_API_KEY=your_moonshot_api_key_here

# 工具配置
ENABLE_WEB_SEARCH=true
ENABLE_CODE_EXECUTION=false
ENABLE_FILE_OPERATIONS=true

# 安全配置
SECRET_KEY=your_secret_key_here
ACCESS_TOKEN_EXPIRE_MINUTES=30

# 外部服务
GATEWAY_SERVICE_URL=http://localhost:8080
KNOWLEDGE_SERVICE_URL=http://localhost:8082
MODEL_SERVICE_URL=http://localhost:8083
BASE_SERVICE_URL=http://localhost:8084

# 监控配置
ENABLE_METRICS=true
METRICS_PORT=9090

# CORS配置
ALLOWED_ORIGINS=http://localhost:3000,http://127.0.0.1:3000
